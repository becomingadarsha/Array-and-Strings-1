#include<stdio.h>
int main()
{
	int i,avg,n,j,k,a[100],larg;
	printf("How many numbers to be inputed ?");
	scanf("%d",&n);
	
		for(i=0;i<n;i++)
		{
			printf("a[%d]=",i);
			scanf("%d",&a[i]);
		}
	larg=a[0];
	for(i=1;i<n;i++)
	{
		if(larg<a[i])
		{
			larg=a[i];
		}
	}
	printf("The largest is %d",larg);
	return 0;
}
